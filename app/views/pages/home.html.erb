<div class="page-wrapper">
  <div class="homepage-wrapper">
    <h1>MAAAG</h1>
    <div class="homepage-content">
      <p>The internet's best curated selection of indie magazines.</p>
      <br>
    </div>
    <div class="homepage-content">
      <div class="button-no-fill">
      <%= link_to "START BROWSING", new_user_session_path %>
      </div>
    </div>
  </div>
</div>
