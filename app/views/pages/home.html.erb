    <div class="magcard" style="background-image: url('<%= image_url(@magazines[3]) %>')"></div>
    <div class="magcard" style="background-image: url('<%= image_url(@magazines[4]) %>')"></div>
    <div class="magcard" style="background-image: url('<%= image_url(@magazines[5]) %>')"></div>
  </div>
  <div class="magcard-row">
    <div class="magcard" style="background-image: url('<%= image_url(@magazines[6]) %>')"></div>
    <div class="magcard" style="background-image: url('<%= image_url(@magazines[7]) %>')"></div>
    <div class="magcard" style="background-image: url('<%= image_url(@magazines[8]) %>')"></div>
  </div>
</div>

